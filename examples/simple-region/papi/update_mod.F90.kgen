MODULE update_mod
    USE calc_mod, only : calc
    PUBLIC update
CONTAINS
    SUBROUTINE update()
        INTEGER :: i, j
        real, dimension(4, 4) :: out2, out3, output
        DO i=1, 4
            DO j=1, 4
                !$kgen begin_callsite calc
                CALL calc(i, j, output, out2, out3)                 !Calling kernel
                !$kgen end_callsite
            END DO
        END DO
        print *, 'SUM(output) = ', SUM(output)
    END SUBROUTINE
END MODULE
